import mysql.connector

# 데이터베이스 연결 설정
connect = mysql.connector.connect(
    user='tester',
    password='blaster_09',
    host='127.0.0.1',
    database='solution',
)

if connect.is_connected():
    cursor = connect.cursor()

    # 'result' 테이블에서 모든 이메일 주소를 가져오는 SELECT 쿼리 실행
    select_query = "SELECT email FROM result"
    cursor.execute(select_query)
    emails = cursor.fetchall()

    # 가져온 이메일 주소들을 출력
    for email in emails:
        print(email[0])

    # 데이터베이스 연결 종료
    connect.close()
